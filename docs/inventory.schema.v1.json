{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://sknups.github.io/schema/inventory.schema.v1.json",
    "title": "inventory",
    "description": "Describes the inventory available to SKNAPP",
    "type": "object",
    "properties": {
      "sku": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "remaining": {
              "type": "number",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "remaining"
          ]
        }
      }
    },
    "required": ["sku"]
  }